        <ImplicitUsings>enable</ImplicitUsings>
        <UserSecretsId>f542f82b-eeab-4c66-b000-56e8f24ac317</UserSecretsId>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.AspNetCore.OpenApi" Version="7.0.4" />
